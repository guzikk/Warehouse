<div class='page-header'>
	<h1>Add new product to warehouse</h1>
</div>

<%= form_for(:product, url:{action:'create', id:@product.id}, method: :post ) do |field| %>
	<%= render(partial: 'form', locals:{product: field})%>
	<%= link_to('Back', {action: 'index'}, class: 'btn btn-warning')%>
	<%= submit_tag('Create', class:'btn btn-primary')%>
<%end%>
